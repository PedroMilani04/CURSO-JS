var cont = 1 
while (cont <= 5) { // While
    console.log(`While Funcionando! Passo ${cont}`)
    cont++ // Incrementa a variavel de 1 em 1
}

var con = 1
do { // Do While (Inversa)
    console.log(`Do While Funcionando! Passo ${con}`)
    con++
} while (con <= 10)

for (var cons = 1; cons <= 15; cons++) {
    console.log(`For Funcionando! Passo ${cons}`)
}